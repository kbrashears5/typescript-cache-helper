{
    "name": "typescript-cache-helper",
    "version": "1.0.0",
    "description": "Typescript Cache Helper",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kbrashears5/typescript-cache-helper.git"
    },
    "bugs": {
        "url": "https://github.com/kbrashears5/typescript-cache-helper/issues"
    },
    "homepage": "https://github.com/kbrashears5/typescript-cache-helper#readme",
    "author": "Kevin Brashears",
    "license": "MIT",
    "keywords": [
        "javascript",
        "typescript",
        "cache",
        "helper"
    ],
    "main": "lib/index.js",
    "dependencies": {
        "node-cache": "5.1.2",
        "typescript-helper-functions": "1.0.34"
    },
    "devDependencies": {
        "@types/jest": "26.0.9",
        "@types/node": "14.0.27",
        "jest": "26.3.0",
        "jest-junit": "11.1.0",
        "rimraf": "3.0.2",
        "ts-jest": "26.1.4",
        "tslint": "6.1.3",
        "typescript": "3.9.7"
    },
    "files": [
        "LICENSE",
        "lib/**"
    ],
    "jest": {
        "preset": "ts-jest",
        "reporters": [
            "default",
            "jest-junit"
        ],
        "rootDir": "src",
        "testEnvironment": "node",
        "coverageReporters": [
            "cobertura"
        ]
    },
    "scripts": {
        "prebuild": "rimraf ./lib",
        "build": "./node_modules/.bin/tsc",
        "clean": "rimraf ./lib",
        "prepublishOnly": "npm run build",
        "postpublish": "npm run clean",
        "test": "jest --ci --coverage"
    }
}
